<h2>All Cookies</h2>
{{log model}}

<div class="row">
  {{#each model as |item|}}
  <div class="col-sm-4 well">
    <h3 class="cookie-name">{{item.name}}</h3>
    <p class="cookie-count">Count: {{item.count}}</p>
    <p class="cookie-price">Price: ${{item.price}}</p>
    <p class="cookie-per-dozen">Per Dozen: ${{item.perDozen}}</p>
  </div>
  {{/each}}
</div>

{{outlet}}
